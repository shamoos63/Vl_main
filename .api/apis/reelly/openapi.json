                          "min_price": {
                            "type": "number"
                          },
                          "min_price_per_area_unit": {
                            "type": "number"
                          },
                          "max_price": {
                            "type": "number"
                          },
                          "price_currency": {
                            "type": "string"
                          },
                          "area_unit": {
